             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="/View/Window/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Converters.xaml" />

                <ResourceDictionary Source="Resources/Style/BorderStyle.xaml" />
                <ResourceDictionary Source="Resources/Style/ButtonStyle.xaml" />
                <ResourceDictionary Source="Resources/Style/TextBlockStyle.xaml" />
                <ResourceDictionary Source="Resources/Style/TextBoxStyle.xaml" />
                <ResourceDictionary Source="Resources/Style/CommonBrushes.xaml" />
                <ResourceDictionary Source="Resources/Style/KeyboardStyle.xaml" />

                <ResourceDictionary Source="Resources/Template/ButtonTemplate.xaml" />
                <ResourceDictionary Source="Resources/Template/Icons.xaml" />
                <ResourceDictionary Source="Resources/Template/ItemsControlTemplate.xaml" />
                <ResourceDictionary Source="Resources/Template/TextBoxTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>